- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\nimport socket\nimport\
      \ os\n\nclass blk(gr.sync_block):\n    \"\"\"\n    Sends FFT vectors via UDP\
      \ only if active=True.\n    \"\"\"\n    def __init__(self, vector_len=2048,\
      \ active=True):\n        gr.sync_block.__init__(\n            self,\n      \
      \      name='UDP Vector Sender',\n            in_sig=[(np.float32, vector_len)],\n\
      \            out_sig=None\n        )\n\n        self.active = active\n\n   \
      \     self.host = os.environ.get(\"BACKEND_HOST\", \"backend-controller\")\n\
      \        try:\n            self.port = int(os.environ.get(\"BACKEND_GRAPHICS_PORT\"\
      , \"9001\"))\n        except ValueError:\n            self.port = 9001\n\n \
      \       self.sock = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)\n     \
      \   self.dest = (self.host, self.port)\n\n    def work(self, input_items, output_items):\n\
      \        in0 = input_items[0]\n        n_items = len(in0)\n\n        if not\
      \ self.active:\n            return n_items\n\n        for vector in in0:\n \
      \           try:\n                self.sock.sendto(vector.tobytes(), self.dest)\n\
      \            except Exception:\n                pass\n\n        return n_items\n"
    active: is_active
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
    vector_len: fft_size
  states:
    _io_cache: ('UDP Vector Sender', 'blk', [('vector_len', '2048'), ('active', 'True')],
      [('0', 'float', 2048)], [], '\nSends FFT vectors via UDP only if active=True.\n',
      ['active'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [752, 384.0]
    rotation: 0
