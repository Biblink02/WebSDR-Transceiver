- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\nimport socket\nimport\
      \ os\n\nclass blk(gr.sync_block):\n    \"\"\"\n    UDP sender that can be muted\
      \ via the 'active' parameter.\n    \"\"\"\n    def __init__(self, active=True):\n\
      \        gr.sync_block.__init__(\n            self,\n            name='UDP Sender\
      \ with Header',\n            in_sig=[np.float32],\n            out_sig=None\n\
      \        )\n        \n        self.active = active\n\n        self.pod_name\
      \ = os.environ.get(\"POD_NAME\", \"worker-manual\")\n        self.host = os.environ.get(\"\
      BACKEND_HOST\", \"backend-controller\")\n        try:\n            self.port\
      \ = int(os.environ.get(\"BACKEND_AUDIO_PORT\", \"9002\"))\n        except ValueError:\n\
      \            self.port = 9002\n            \n        self.sock = socket.socket(socket.AF_INET,\
      \ socket.SOCK_DGRAM)\n        self.dest = (self.host, self.port)\n        self.header\
      \ = f\"{self.pod_name}:\".encode('utf-8')\n\n    def work(self, input_items,\
      \ output_items):\n        in0 = input_items[0]\n        n_items = len(in0)\n\
      \        \n        if not self.active:\n            return n_items\n\n     \
      \   if n_items > 0:\n            try:\n                audio_payload = in0.tobytes()\n\
      \                self.sock.sendto(self.header + audio_payload, self.dest)\n\
      \            except Exception:\n                pass\n                \n   \
      \     return n_items\n"
    active: is_active
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('UDP Sender with Header', 'blk', [('active', 'True')], [('0', 'float',
      1)], [], "\nUDP sender that can be muted via the 'active' parameter.\n", ['active'])
    bus_sink: false
